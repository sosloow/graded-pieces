<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <title>Graded pieces</title>
  <link rel="stylesheet" href="https://fonts.googleapis.com/icon?family=Material+Icons">
  <link rel="stylesheet" href="https://code.getmdl.io/1.1.3/material.indigo-pink.min.css">
  <script defer src="https://code.getmdl.io/1.1.3/material.min.js"></script>
  <style>
    #pieces-table td {
      white-space: normal;
    }
  </style>
</head>
<body>
  <div class="mdl-layout mdl-js-layout">
    <main class="mdl-layout__content">
      <div class="page-content">
        <table id="pieces-table" class="mdl-data-table mdl-js-data-table">
          <thead>
            <tr>
              <th>id</th>
              <th>grade</th>
              <th class="mdl-data-table__cell--non-numeric">composer</th>
              <th class="mdl-data-table__cell--non-numeric">piece</th>
              <th class="mdl-data-table__cell--non-numeric">Main Technical Difficulty or Benefit</th>
              <th class="mdl-data-table__cell--non-numeric">Other Notes & Comments</th>
            </tr>
          </thead>
          <tbody>
            <% pieces.forEach(piece => { %>
              <tr>
                <td><%= piece.id %></td>
                <td><%= piece.grade %></td>
                <td class="mdl-data-table__cell--non-numeric"><%= piece.composer %></td>
                <td class="mdl-data-table__cell--non-numeric"><%= piece.name %></td>
                <td class="mdl-data-table__cell--non-numeric"><%= piece.benefits %></td>
                <td class="mdl-data-table__cell--non-numeric"><%= piece.info %></td>
              </tr>
            <% }); %>
          </tbody>
        </table>
      </div>
    </main>
  </div>
</body>
</html>
